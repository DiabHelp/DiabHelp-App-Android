        <activity android:name=".Suivi_proches_patients.procheItemActivity"
            android:parentActivityName=".Suivi_proches_patients.ListeProches"
            android:screenOrientation="portrait">
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Suivi_proches_patients.ListeProches" />
        </activity>
    </application>

</manifest>
