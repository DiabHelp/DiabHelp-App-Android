<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.diabhelp.carnetdesuivi" >

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Carnetdesuivi"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Carnet.EntryActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan"
            >
        </activity>
        <activity
            android:name=".Carnet.AccueilStatistics"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan"
            >
        </activity>
        <activity android:name=".Carnet.DayResultActivity"
            android:label="@string/app_name">

        </activity>
        <activity
            android:name=".Carnet.Statistics.StatisticsActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan"
            >
        </activity>

    </application>

</manifest>
