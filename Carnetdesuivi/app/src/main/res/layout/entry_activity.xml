<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:layout_gravity="center"
    android:orientation="vertical"
    >

    <!--    <ListView
            android:id="@+id/listView"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:descendantFocusability="beforeDescendants"
            >
        </ListView>-->
    <!--    android:layout_marginBottom="10dp" ds le bouton-->
    <Button
        android:id="@+id/SignUpButton"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#078b87"
        android:onClick="save_button"
        android:text="Ajouter"
        android:textAllCaps="true"
        android:textColor="@color/abc_secondary_text_material_dark"
        android:textStyle="bold"

        />
    <!--    <GridView
            android:numColumns="auto_fit"
            android:gravity="center"
            android:columnWidth="100dp"
            android:stretchMode="columnWidth"
            android:layout_width="wrap_content"
            android:layout_height="120dp"
            android:scrollbars="horizontal"
            android:layout_below="@+id/SignUpButton"
            android:id="@+id/grid"

            />-->

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grid"
        android:layout_gravity="center"
        android:layout_margin="@dimen/activity_horizontal_margin"
        android:paddingTop="7dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            android:orientation="vertical">
            <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:columnCount="3"
                android:orientation="vertical"
                android:rowCount="12" >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:id="@+id/titreview"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="Titre"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold" />
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"/>
                    <EditText
                        android:id="@+id/edittitre"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:inputType="text" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:id="@+id/dateview"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="Date"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold"/>
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"/>
                    <EditText
                        android:id="@+id/editDate"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:focusable="false"
                        android:inputType="none"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10"
                    >
                    <TextView
                        android:id="@+id/placeview"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="Lieux"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold" />
                    <ImageView
                        android:id="@+id/dateimg"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/heigh_img_entry_activity"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:src="@drawable/ic_place_black_48dp"/>
                    <EditText
                        android:id="@+id/editplace"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:id="@+id/GlycemyView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="Glycemie (mg/dl)"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold"/>
                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/heigh_img_entry_activity"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:src="@drawable/glycemiegood"/>
                    <EditText
                        android:id="@+id/editglycemy"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:inputType="numberDecimal"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:id="@+id/glucideview"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="Glucides (g))"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold"/>
                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/heigh_img_entry_activity"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:src="@drawable/glucide"/>
                    <EditText
                        android:id="@+id/editglucide"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:inputType="numberDecimal"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:id="@+id/placeactivity"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="Durée d'activité"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold"/>
                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/heigh_img_entry_activity"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:src="@drawable/sport2"/>
                    <EditText
                        android:id="@+id/editactivity"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:focusable="false"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:id="@+id/activiytypeview"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="Type d'activité"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold"/>
                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/heigh_img_entry_activity"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:src="@drawable/sport2"/>
                    <EditText
                        android:id="@+id/editactivitytype"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:inputType="text"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:id="@+id/rapideview"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="Rapide (unitées)"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold"/>
                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/heigh_img_entry_activity"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:src="@drawable/fast"/>
                    <EditText
                        android:id="@+id/editrapide"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:inputType="numberDecimal"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:id="@+id/lenteview"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="Lente (unitées)"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold"/>
                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/heigh_img_entry_activity"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:src="@drawable/slow"/>
                    <EditText
                        android:id="@+id/editlente"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:inputType="numberDecimal"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:id="@+id/hba1cview"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="HbA1c"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold"/>
                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/heigh_img_entry_activity"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:src="@drawable/hba1c"/>
                    <EditText
                        android:id="@+id/edithba1c"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:inputType="numberDecimal"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:id="@+id/Notesview"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="Notes"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textStyle="bold"/>
                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/heigh_img_entry_activity"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"/>
                    <EditText
                        android:id="@+id/editnotes"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="5"
                        android:inputType="textMultiLine"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/tag_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Tags"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textStyle="bold"/>
            </GridLayout>
            <HorizontalScrollView
                android:id="@+id/horizontalScrollView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp">
                <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:columnCount="7"
                    android:orientation="vertical"
                    android:rowCount="2">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="15dp"
                        android:layout_row="0"
                        android:onClick="add_breakfeast"
                        android:orientation="vertical"
                        >

                        <ImageView
                            android:id="@+id/imgptidej"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/ptidej"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtptidej"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/petitdej"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>
                    </LinearLayout>

                    <!--Déjeuner-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_row="0"
                        android:onClick="add_launch"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imglaunch"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/lunch"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtlaunch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/launch"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>
                    </LinearLayout>

                    <!--Diner-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="0"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="15dp"
                        android:layout_row="1"
                        android:onClick="add_diner"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imgdiner"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/diner"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtdiner"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="15dp"
                            android:text="@string/diner"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>
                    </LinearLayout>

                    <!--Encas-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="1"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_row="1"
                        android:onClick="add_encas"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imgencas"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/cassecroute"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtencas"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/encas"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>
                    </LinearLayout>

                    <!--coucher-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="2"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_row="0"
                        android:onClick="add_sleep"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imgcoucher"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/coucher"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtcoucher"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/sleep"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>

                    </LinearLayout>

                    <!--levée-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="2"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_row="1"
                        android:onClick="add_wakeup"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imglevee"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/reveil"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtlevee"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/wakeup"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>

                    </LinearLayout>

                    <!--nuit-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="3"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_row="0"
                        android:onClick="add_night"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imgnuit"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/night"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtnuit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/night"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>

                    </LinearLayout>

                    <!--Sport-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="3"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_row="1"
                        android:onClick="add_workout"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imgsport"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/sport"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtsport"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/workout"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>

                    </LinearLayout>

                    <!--hypo-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="4"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_row="0"
                        android:onClick="add_hypo"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imghypo"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/hypo"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txthypo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/hypoglycemi"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>

                    </LinearLayout>

                    <!--hyper-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="4"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_row="1"
                        android:onClick="add_hyper"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imghyper"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/hyper"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txthyper"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/hyperglycemi"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>

                    </LinearLayout>

                    <!--Travail-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="5"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_row="0"
                        android:onClick="add_work"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imgwork"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/work"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtwork"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/work"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>

                    </LinearLayout>

                    <!--Home-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="5"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_row="1"
                        android:onClick="add_home"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imghome"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/home"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txthome"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/home"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>

                    </LinearLayout>

                    <!--Alcohol-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="6"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_marginRight="10dp"
                        android:layout_row="0"
                        android:onClick="add_alcohol"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imgalcool"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/alcool"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtalcool"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/alcohol"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>

                    </LinearLayout>

                    <!--Period-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_column="6"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                        android:layout_marginLeft="@dimen/entry_icone_marginLeft"
                        android:layout_marginRight="10dp"
                        android:layout_row="1"
                        android:onClick="add_period"
                        android:orientation="vertical"
                        >
                        <ImageView
                            android:id="@+id/imgperiod"
                            android:layout_width="@dimen/img_entry_size_width"
                            android:layout_height="@dimen/img_entry_size_height"
                            android:src="@drawable/period"
                            >
                        </ImageView>

                        <TextView
                            android:id="@+id/txtperiod"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/txt_entry_marginTop"
                            android:text="@string/period"
                            android:textSize="@dimen/text_entry_size"
                            >
                        </TextView>
                    </LinearLayout>
                </GridLayout>
            </HorizontalScrollView>
        </LinearLayout>
    </ScrollView>
</LinearLayout>