                    <ImageView
                        android:id="@+id/imgperiod"
                        android:layout_width="@dimen/img_entry_size_width"
                        android:layout_height="@dimen/img_entry_size_height"
                        android:src="@drawable/period" />

                    <TextView
                        android:id="@+id/txtperiod"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/txt_entry_marginTop"
                        android:textSize="@dimen/text_entry_size"
                        android:text="@string/period"
                        android:layout_gravity="center" />
                </LinearLayout>

            </GridLayout>

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@id/gridLayout3"
                android:layout_marginTop="15dp"
                android:id="@+id/gridLayout4" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="@dimen/margin_left_icone_resultday"
                    android:layout_marginBottom="@dimen/entry_icone_marginBottom"
                    android:layout_gravity="center"
                    android:id="@+id/layout_sport2" >

                    <ImageView
                        android:id="@+id/imgsport2"
                        android:layout_width="@dimen/img_entry_size_width"
                        android:layout_height="@dimen/img_entry_size_height"
                        android:src="@drawable/sport2" />

                    <TextView
                        android:id="@+id/ndheuresport"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/txt_entry_marginTop"
                        android:textSize="@dimen/text_entry_size"
                        android:text="Sport : 5H00"
                        android:textStyle="bold"
                        android:layout_gravity="center" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:id="@+id/linearLayout2" >

                    <TextView
                        android:id="@+id/txtsportdesc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_entry_size"
                        android:visibility="gone"
                        android:text="Détail :"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:layout_column="1"
                        android:layout_row="5" />

                    <TextView
                        android:id="@+id/txtsport2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/text_entry_size"
                        android:text="TEST"
                        android:textStyle="italic"
                        android:layout_gravity="center"
                        android:layout_column="2"
                        android:layout_row="5" />
                </LinearLayout>

            </GridLayout>

            <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/layoutdetail"
            android:layout_below="@+id/gridLayout4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:gravity="center"
            android:layout_marginBottom="20dp"
            android:visibility="gone"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Détails"
                android:textStyle="bold"
                android:id="@+id/Detail"
                android:layout_centerHorizontal="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Large Text"
                android:id="@+id/detailfull"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="15dp"
                />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:id="@+id/layoutbutton"
            android:layout_below="@+id/layoutdetail">

            <Button
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Modifier"
                android:onClick="updateEntry"
                android:textAllCaps="true"
                android:id="@+id/UpdateEntryButton"
                android:textColor="@color/abc_secondary_text_material_dark"
                android:background="#078b87"
                android:layout_row="0"
                android:layout_column="0"
                android:layout_marginBottom="5dp"
                />

            <Button
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Supprimer"
                android:onClick="DeleteEntry"
                android:textAllCaps="true"
                android:id="@+id/deleteentryButton"
                android:textColor="@color/abc_secondary_text_material_dark"
                android:background="#078b87"
                android:layout_row="1"
                android:layout_column="0"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_above="@+id/UpdateEntryButton"
                />

        </LinearLayout>


        </RelativeLayout>
    </ScrollView>

</RelativeLayout>